{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "EventHub": {
    "BootstrapServers": "localhost:9092",
    "GroupId": "consumer-connect",
    "EnableAutoCommit": true,
    "StatisticsIntervalMS": 5000,
    "SessionTimeOutMS": 6000,
    "AutoOffsetReset": 0,
    "EnablePartitionOf": true
  }
}
